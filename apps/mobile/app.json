{
  "expo": {
    "name": "Stunity",
    "slug": "stunity-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "stunity",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/Stunity.png",
    "splash": {
      "image": "./assets/Stunity.png",
      "resizeMode": "contain",
      "backgroundColor": "#F59E0B"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.stunity.mobile",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "Stunity needs camera access to take photos for posts and profile pictures.",
        "NSPhotoLibraryUsageDescription": "Stunity needs photo library access to share photos in posts.",
        "NSMicrophoneUsageDescription": "Stunity needs microphone access to record videos.",
        "NSFaceIDUsageDescription": "Stunity uses Face ID for secure login."
      }
    },
    "android": {
      "package": "com.stunity.mobile",
      "googleServicesFile": "./google-services.json",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/Stunity.png",
        "backgroundColor": "#F59E0B"
      },
      "splash": {
        "image": "./assets/Stunity.png",
        "resizeMode": "contain",
        "backgroundColor": "#F59E0B"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-dev-client",
      "expo-secure-store",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#F59E0B",
          "image": "./assets/Stunity.png",
          "imageWidth": 200
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Stunity to access your camera."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Stunity to access your photos."
        }
      ],
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/Stunity.png",
          "color": "#F59E0B"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "b71fa713-5e27-4f79-8f3f-7508834fe005"
      }
    },
    "owner": "naingseiha"
  },
  "main": "index.js"
}